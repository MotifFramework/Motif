{
  "name": "motif",
  "version": "4.0.0",
  "description": "Motif: a responsive front-end framework that supports you",
  "main": "index.js",
  "preCSS": "less",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/MotifFramework/Motif.git"
  },
  "author": "Lifeblue",
  "license": "MIT Open Source",
  "gitHead": "cce044bb9b91bffe6cddd736bc925a22b58fbcd8",
  "engines": {
    "node": ">= 8.11.0",
    "npm": ">= 6.x.x"
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "useBuiltIns": "usage"
        }
      ]
    ]
  },
  "browser": {
    "jquery": "./js/vendor/jquery-3.1.1.js",
    "lazysizes": "./js/vendor/lazysizes.js",
    "picturefill": "./js/vendor/picturefill.js",
    "raf": "./js/vendor/requestAnimFrame.js",
    "motif-reveal": "./js/ui/motif.reveal.js",
    "motif-tabs": "./js/ui/motif.tabs.js",
    "motif-herald": "./js/scroll/motif.herald.js",
    "motif-gauntlet": "./js/forms/motif.gauntlet.js",
    "motif-ajaxsubmission": "./js/forms/motif.ajax-submission.js",
    "motif-plugins": "./js/utils/motif.utils.plugins.js"
  },
  "browserify": {
    "debug": true,
    "transform": [
      "browserify-shim",
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "usage"
              }
            ]
          ]
        }
      ]
    ]
  },
  "browserify-shim": {
    "jquery": "$",
    "raf": "requestAnimFrame",
    "lazysizes": "lazysizes",
    "picturefill": "picturefill",
    "motif-reveal": {
      "depends": [
        "jquery:$",
        "raf:requestAnimFrame"
      ]
    },
    "motif-tabs": {
      "depends": [
        "jquery:$",
        "raf:requestAnimFrame",
        "motif-plugins",
        "motif-reveal"
      ]
    },
    "motif-herald": {
      "depends": [
        "jquery:$"
      ]
    },
    "motif-gauntlet": {
      "depends": [
        "jquery:$",
        "motif-plugins"
      ]
    },
    "motif-ajaxsubmission": {
      "depends": [
        "jquery:$",
        "motif-plugins",
        "motif-gauntlet"
      ]
    },
    "motif-plugins": {
      "depends": [
        "jquery:$"
      ]
    }
  },
  "dependencies": {
    "@babel/core": "^7.1.0",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.1.5",
    "babelify": "^10.0.0",
    "browserify-shim": "^3.8.14",
    "cssnano": "^4.1.4",
    "grunt": "^1.0.3",
    "grunt-browserify": "^5.3.0",
    "grunt-cachebuster": "^0.1.7",
    "grunt-cli": "^1.3.1",
    "grunt-concurrent": "^2.3.1",
    "grunt-contrib-less": "^2.0.0",
    "grunt-contrib-uglify": "^4.0.0",
    "grunt-contrib-watch": "^1.1.0",
    "grunt-modernizr": "^1.0.3",
    "grunt-node-version": "^1.0.2",
    "grunt-postcss": "^0.9.0",
    "grunt-svgmin": "^5.0.0",
    "grunt-svgstore": "^1.0.0",
    "load-grunt-config": "^0.19.2",
    "time-grunt": "^1.4.0"
  }
}
